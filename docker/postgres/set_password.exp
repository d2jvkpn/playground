#! /usr/bin/env expect
set prompt "#"
set timeout 60

# set container [lindex $argv 1];

set fh [open "./configs/postgres.secret" r]
set password [read -nonewline $fh]
close $fh

# --password
spawn docker exec -it -u postgres ${CONAINTER} psql -x -c '\password postgres'
expect "Enter new password for user \"postgres\":"
send "$password\n"
interact
